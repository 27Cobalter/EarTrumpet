    <SolidColorBrush x:Key="HyperlinkTextForeground">Green</SolidColorBrush>
    <SolidColorBrush x:Key="HyperlinkTextForegroundHover">Grey</SolidColorBrush>

    <BitmapImage x:Key="LogoImage" UriSource="pack://application:,,,/EarTrumpet;component/Assets/Logo-Dark.png"></BitmapImage>


</ResourceDictionary>